import os

# Função com retorno
def logoSenai():
    os.system("clear")
    print("=== SENAI ===")

# Função com retorno.
def somar(n1,n2):
    resultado = n1 + n2
    return resultado

# Solicitando dados para o usuário.
logoSenai()
primeiroNumero = int(input("Digite o Primeiro Número: "))
segundoNumero = int(input("Digite o Segundo Número: "))

soma = somar(primeiroNumero,segundoNumero)

# Exibindo dados.

print(f"Soma: {soma}")
